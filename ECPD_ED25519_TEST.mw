M := 2^255-19: 
d := 37095705934669439343138083508754565189542113879843219016388785533085940283555:
n(1) := 2^66-1: n(2) := 2^66-5: n(3) := 2^66-9: n(4) := 2^66-17: n(5) := 2^66-33: n(6) := 2^66-65: n(7) := 2^66-257: n(8) := 2^66-513:
Dn := 1: for i to 8 do Dn := Dn*n(i) end do: for i to 8 do DI(i) := Dn/n(i) end do: for i to 8 do DIinv(i) := modp(1/DI(i), n(i)) end do:
for i from 1 to 8 do  Xn(i):= x1 mod n(i):  Y(i):= y1 mod n(i): end do:
SOMR:= proc(X) 
local Dn,i,Al2,alpha,KZS,j,Gamma:
for i to 8 do Gamma(i) := modp((modp(X, n(i)))*DIinv(i), n(i)) end do:
Al2 := 0; for i to 8 do Al2 := Al2+Gamma(i)/n(i) end do: alpha := floor(Al2): 
KZS := 0; for i to 8 do KZS := KZS+Gamma(i)*(modp(DI(i), M))/M end do; KZS := trunc(KZS):
for i to 8 do Ki(i) := modp(KZS, n(i)) end do:
for i to 8 do Mi(i) := modp(-M, n(i)) end do: for i to 8 do KM(i) := modp(Ki(i)*Mi(i), n(i)) end do:
for i to 8 do for j to 8 do (DI_M_mj(i))(j) := modp(modp(DI(i), M), n(j)) end do: end do:
for i to 8 do for j to 8 do (Y(i))(j) := modp(Gamma(i)*(DI_M_mj(i))(j), n(j)) end do: end do:
for i to 8 do sigma(i) := 0 end do; for i to 8 do for j to 8 do sigma(i) := modp(sigma(i)+(Y(j))(i), n(i)) end do: end do:
for i to 8 do alphaD(i) := modp(modp(-alpha*Dn, M), n(i)) end do:
for i to 8 do Z(i) := modp(sigma(i)+alphaD(i), n(i)) end do:
for i to 8 do z(i) := modp(Z(i)+KM(i), n(i)) end do: printf(&quot;\134n&quot;); for i to 8 do printf(&quot;%X \134t &quot;, z(i)); end do:
