local PDX, M, zetta, etta; M := 115792089237316195423570985008687907853269984665640564039457584007908834671663; 
zetta := modp(3*x1^2+0*z1^4, M); etta := modp(4*x1*y1^2, M); PDX := modp(zetta^2-2*etta, M);
#printf(&quot;%d&quot;,PDX);
end proc:
PDY := proc (x1, y1, z1) 
local PDY, PDX, M, zetta, etta; M := 115792089237316195423570985008687907853269984665640564039457584007908834671663; 
zetta := modp(3*x1^2+0*z1^4, M); etta := modp(4*x1*y1^2, M); PDX := modp(zetta^2-2*etta, M); PDY := modp(modp((modp(zetta*(etta-PDX), M))-8*y1^4, M), M); 
