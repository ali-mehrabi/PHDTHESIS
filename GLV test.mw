</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" inline-output="false" labelreference="L9" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion3" expanded="true" visible="true" pixel-width="787" pixel-height="3804" code-language="text/maple" autoexpand="true" wrapping="false" show-border="true" code-line-numbers="true"># CALCULATE USING GLV
l:= 37718080363155996902926221483475020450927657555482586988616620542887997980018:
b:= 55594575648329892869085402983802832744385952214688224221778511981742606582254:
p := 2^256-2^32-2^9-2^8-2^7-2^6-2^4-1:

# input POINT P
X1 := 56576513649176532955305617254616790498672209379484940581393603843805619269570:
Y1 := 39155707150128334349216371677407456506802956851096117747929288260567018884059:
Z1 := 65341020041517633956166170261014086368942546761318486551877808671514674964848:
# POINT Q: 
X2:= X1*b mod p:
Y2:= Y1:
Z2:= Z1:
# Calculate  P+Q
X3:= PAX(X1,Y1,Z1,X2,Y2,Z2):
Y3:= PAY(X1,Y1,Z1,X2,Y2,Z2):
Z3:= PAZ(X1,Y1,Z1,X2,Y2,Z2):
#POINT  -P :
X4:= X1:
Y4:= -Y1 mod p:
Z4:= Z1:
#POINT -Q:
X5:= X2:
Y5:= -Y2 mod p:
Z5:= Z2:
#Calculate P-Q:
X6:= PAX(X1,Y1,Z1,X5,Y5,Z5):
Y6:= PAY(X1,Y1,Z1,X5,Y5,Z5):
Z6:= PAZ(X1,Y1,Z1,X5,Y5,Z5):
#Calculate -P+Q
X7:= PAX(X2,Y2,Z2,X4,Y4,Z4):
Y7:= PAY(X2,Y2,Z2,X4,Y4,Z4):
Z7:= PAZ(X2,Y2,Z2,X4,Y4,Z4):
#Calculate -(P+Q)
X8:= X3:
Y8:= -Y3 mod p:
Z8:= Z3:

#################################################################
SX:=0:
SY:=0:
SZ:=0:
#################################################################
if ii&gt;jj then 
	kk:= jj-1:
elif jj&gt;ii then
	kk:= ii-1:
else
	kk:= ii-1:	
end if:
		
if ii &gt; jj then 
	SX:=X1:
     SY:=Y1:
     SZ:=Z1:
	for i from ii-2 to kk by -1 do 
	    if ZZ(i) = 1 then  # 2P +P
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ):	  
          SX:= PAX(SX1,SY1,SZ1,X1,Y1,Z1):
          SY:= PAY(SX1,SY1,SZ1,X1,Y1,Z1):
          SZ:= PAZ(SX1,SY1,SZ1,X1,Y1,Z1):
          elif ( ZZ(i) = -1) then #2P-P
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ):	  
          SX:= PAX(SX1,SY1,SZ1,X4,Y4,Z4):
          SY:= PAY(SX1,SY1,SZ1,X4,Y4,Z4):
          SZ:= PAZ(SX1,SY1,SZ1,X4,Y4,Z4):
          else  # 2P
          SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ):
          SX:=SX1: SY:=SY1: SZ:=SZ1:	
          end if:            	 
     end do:
	     
elif jj&gt; ii then
     SX:=X2:
     SY:=Y2:
     SZ:=Z2:	 
	for i from jj-2 to kk by -1 do 
	    if ZX(i) = 1 then  # 2Q+Q
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ):	  
          SX:= PAX(SX1,SY1,SZ1,X2,Y2,Z2):
          SY:= PAY(SX1,SY1,SZ1,X2,Y2,Z2):
          SZ:= PAZ(SX1,SY1,SZ1,X2,Y2,Z2):
          elif (ZX(i) = -1) then #2Q-Q
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ):	  
          SX:= PAX(SX1,SY1,SZ1,X5,Y5,Z5):
          SY:= PAY(SX1,SY1,SZ1,X5,Y5,Z5):
          SZ:= PAZ(SX1,SY1,SZ1,X5,Y5,Z5):
          else
          SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ):
          SX:=SX1: SY:=SY1: SZ:=SZ1:	
          end if:            	 
     end do:
	     
end if;
if ii&lt; jj then: 
SX1:= PAX(SX,SY,SZ,X1,Y1,Z1):
SY1:= PAY(SX,SY,SZ,X1,Y1,Z1):
SZ1:= PAZ(SX,SY,SZ,X1,Y1,Z1):
elif jj &lt; ii then 
SX1:= PAX(SX,SY,SZ,X2,Y2,Z2):
SY1:= PAY(SX,SY,SZ,X2,Y2,Z2):
SZ1:= PAZ(SX,SY,SZ,X2,Y2,Z2):
end if:	
SX:=SX1: 
SY:=SY1: 
SZ:=SZ1:	
     
for j from kk-1 to 0 by -1 do
	if ( ZZ(j) = 1 and ZX(j)=1 ) then # 2(P+Q) +P+Q
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X3,Y3,Z3):
          SY:=  PAY(SX1,SY1,SZ1,X3,Y3,Z3):
          SZ:=  PAZ(SX1,SY1,SZ1,X3,Y3,Z3):  
     elif  ( ZZ(j) = 1 and ZX(j)=-1 ) then #2(P+Q) +P-Q
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X6,Y6,Z6):
          SY:=  PAY(SX1,SY1,SZ1,X6,Y6,Z6):
          SZ:=  PAZ(SX1,SY1,SZ1,X6,Y6,Z6): 
     elif  ( ZZ(j) = -1 and ZX(j)=-1 ) then 
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X8,Y8,Z8):
          SY:=  PAY(SX1,SY1,SZ1,X8,Y8,Z8):
          SZ:=  PAZ(SX1,SY1,SZ1,X8,Y8,Z8): 
      elif  ( ZZ(j) = -1 and ZX(j)= 1 ) then     
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X7,Y7,Z7):
          SY:=  PAY(SX1,SY1,SZ1,X7,Y7,Z7):
          SZ:=  PAZ(SX1,SY1,SZ1,X7,Y7,Z7): 
      elif  ( ZZ(j) = 0 and ZX(j)= 1 ) then             
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X2,Y2,Z2):
          SY:=  PAY(SX1,SY1,SZ1,X2,Y2,Z2):
          SZ:=  PAZ(SX1,SY1,SZ1,X2,Y2,Z2): 
      elif  ( ZZ(j) = 0 and ZX(j)= -1 ) then             
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X5,Y5,Z5):
          SY:=  PAY(SX1,SY1,SZ1,X5,Y5,Z5):
          SZ:=  PAZ(SX1,SY1,SZ1,X5,Y5,Z5): 
       elif  ( ZZ(j) = 1 and ZX(j)= 0 ) then             
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X1,Y1,Z1):
          SY:=  PAY(SX1,SY1,SZ1,X1,Y1,Z1):
          SZ:=  PAZ(SX1,SY1,SZ1,X1,Y1,Z1): 
      elif  ( ZZ(j) = -1 and ZX(j)= 0 ) then             
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=  PAX(SX1,SY1,SZ1,X4,Y4,Z4):
          SY:=  PAY(SX1,SY1,SZ1,X4,Y4,Z4):
          SZ:=  PAZ(SX1,SY1,SZ1,X4,Y4,Z4):   
      elif  ( ZZ(j) = 0 and ZX(j)= 0 ) then             
		SX1:= PDX(SX,SY,SZ):
		SY1:= PDY(SX,SY,SZ):
          SZ1:= PDZ(SX,SY,SZ): 
          SX:=SX1: SY:=SY1: SZ:=SZ1:                              	
     end if;     
end do:

SX;
SY;
SZ;

AFFINE(SX,SY,SZ);
