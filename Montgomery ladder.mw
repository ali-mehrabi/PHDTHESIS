# NAF equivalent 
local i,j,k,A,B,C,D,E,F,G,Z,Y,P,length;
E:= 2^12-2^11+2^8-2^7-2^3+1:
C:=0:
G:=0:
D:=3.5:
A:= 4.6:
i:=0: 
j:=0:
P:=1:
Q:=0:
while E &gt; 0 do 
	if (modp(E,2)&lt;&gt;0) then 
		Z(i):= 2- modp(E,4);
		E:=E-Z(i);
     else 
     	Z(i):=0;   	
     end if:
     E:= E/2;
     #printf(&quot;Z(%2d)= %d,&quot;,i,Z(i));   
     if (Z(i)=1 or Z(i)=-1) then 
        C:= C+A;
     else 
        C:=C+D;	
     end if:    
        i:=i+1: 
end do: 
     C:=C-A:
     length:=i-1:
printf(&quot;NAF=&quot;):for k from i-1 by -1 to 0 do printf(&quot;%d&quot;,Z(k) ); end do; 
Q:=P:
for j from length-1 by -2 to 0 do 
    if j &gt; 0 then 
    	  if (Z(j)=0 and Z(j-1)=0) then 
    	     Q:=4*Q;
    	  elif (Z(j)=0 and Z(j-1)=1) then   	
          Q:=4*Q+P;
       elif (Z(j)=0 and Z(j-1)=-1) then   
          Q:=4*Q-P;
       elif (Z(j)=1 and Z(j-1)=0) then 
          Q:=4*Q+2*P;
       elif (Z(j)=-1 and Z(j-1)=0) then 
          Q:=4*Q-2*P;	
       end if;
    else
    	 if Z(j)=0  then  
    	     Q:=2*Q;  
    	 elif Z(j) = 1 then 
    	     Q:=2*Q+P;
    	 else
    	     Q:=2*Q-P;
    	 end if;
    end if;
    #printf(&quot;\134nP=%d&quot;,Q);
end do;
printf(&quot;\134nP=%d&quot;,Q);</EC-CodeEditor>
