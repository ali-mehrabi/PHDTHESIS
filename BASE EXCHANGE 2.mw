  toRNS := proc (A::integer, m::list(integer))
    description &quot;represent the integer A in the RNS with moduli m&quot;;
    map((m,x)-&gt;modp(x,m), m, A);
  end proc:

  fromRNS := proc (a::list(integer), m::list(integer))
    local M, Mi, Mhat;
    description &quot;the value of the integer a in the RNS with moduli m&quot;;
    M := mul(m[i],i=1..nops(m));
    Mi := map((x,y)-&gt;y/x, m, M);
    Mhat := zip((x,m)-&gt;modp(1/x,m), Mi, m);
    modp(add(Mi[i]*modp(a[i]*Mhat[i],m[i]),i=1..nops(m)),M);
  end proc:

  addRNS := proc (a::list(integer), b::list(integer), m::list(integer))
    local c;
    description &quot;sum the integers a and b within the RNS with moduli m&quot;;
    c := zip((x,y)-&gt;x+y, a, b);
    zip((x,m)-&gt;modp(x,m), c, m);
  end proc:

  multRNS := proc (a::list(integer), b::list(integer), m::list(integer))
    local c;
    description &quot;multiply the integers a and b within the RNS with moduli m&quot;;
    c := zip((x,y)-&gt;x*y, a, b);
    zip((x,m)-&gt;modp(x,m), c, m);
  end proc:



  subRNS := proc (a::list(integer), b::list(integer), m::list(integer))
    local c;
    description &quot;subtract integer b from a within the RNS with moduli m&quot;;
    c := zip((x,y)-&gt;x-y, a, b);
    zip((x,m)-&gt;modp(x,m), c, m);
  end proc:

  invertRNS := proc (a::list(integer), m::list(integer))
    description &quot;invert integer a within the RNS with moduli m&quot;;
    zip((x,m)-&gt;modp(1/x,m), a, m);
  end proc:

  dynamicrangeRNS := proc (m::list(integer))
    description &quot;dynamic range of the RNS with moduli m&quot;;
    mul(m[i],i=1..nops(m));
  end proc:

  channelsRNS := proc (m::list(integer))
    option inline;
    description &quot;the number of channels (moduli) in the RNS with moduli m&quot;;
    nops(m);
  end proc:

  unitRNS := proc(m::list(integer))
    description &quot;the number 1 in the RNS with moduli m&quot;;
    map((x)-&gt;1,m);
  end proc:

  zeroRNS := proc(m::list(integer))
    description &quot;the number 0 in the RNS with moduli m&quot;;
    map((x)-&gt;0,m);
  end proc:

  scalaropRNS := proc(f,a::list(integer),s)
    description &quot;apply f(a[i],s) to each channel of a with scalar s&quot;;
    map(f,a,s);
  end proc:

  opRNS := proc(f,a::list(integer),b::list(integer))
    description &quot;apply f(a[i],b[i]) channel-by-channel to a and b&quot;;
    zip(f,a,b);
  end proc:

  normaliseRNS := proc(a::list(integer),m::list(integer))
    description &quot;reduce the channels of a modulo m&quot;;
    zip(modp,a,m);
  end proc:

  BERNS := proc(a::list(integer), m::list(integer), mp::list(integer))
    local M, Mhat, Mhatinv, xt;
    description &quot;given a in the RNS m, find the representation of a in the RNS with moduli mp&quot;;
    M := mul(m[i],i=1..nops(m));
    Mhat := map((x,y)-&gt;y, m, M);
    Mhat := zip((x,y)-&gt;x/y, Mhat, m);
    Mhatinv := zip((x,m)-&gt;modp(1/x,m), Mhat, m);
    xt := zip((x,y)-&gt;x*y,Mhatinv,a);
    xt := zip((x,m)-&gt;modp(x,m), xt, m);
    xt := zip((x,y)-&gt;x*y,Mhat,xt);
    xt := modp(add(xt[i],i=1..nops(xt)),M);
    [op(map((m,x)-&gt;modp(x,m), mp, xt))];
  end proc:

  mrsweightsRNS := proc (m::list(integer))
    description &quot;weights of the mixed-radix number system corresponding to the RNS with moduli m&quot;;
    [ seq( mul(m[j],j = 1..(i-1)), i=1..nops(m)) ];
  end proc:

  mrsRNS := proc (a::list(integer), m::list(integer))
    local xt, i, b;
    description &quot;representation of a in mixed-radix system given its representation in the RNS with moduli m&quot;;
    xt := a;
    b := [];
    for i from 1 to (nops(m)-1) do
      b := [op(b), xt[1]];
