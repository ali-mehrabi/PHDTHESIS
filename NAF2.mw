</Group><CodeEditor-ExecGroup view="presentation" hide-input="false" labelreference="L3" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion1" expanded="true" visible="true" pixel-width="1127" pixel-height="2065" code-language="text/maple" autoexpand="false" wrapping="false" show-border="true" code-line-numbers="true">#Obtain costs of scalar multiplication
with(RandomTools):
local i,j,k,l,m,n,A,B,C,D,E,F,G,Z,Y,CC,GG,MC,Cm,MGC,MT,t,BN,BNCC,MGLP,MGLS,NAF;
D:=176:
A:=244:
t:=1:
for m from t to 60 do 
R:=(2^32)*rand()+(2^128)*rand();
E:= 2^256 -R ;
F:= E;
i:= 0: 
j:= 0:
P:=1:
R0:=P:
R1:=2*P:
MGLS:=0:
MGLP:=0;
BNM:=0:
BAD:=0:
NAF:=0:

############################################################
# BINARY COSTS
FF:=F:
while FF&gt;0 do
	if (modp(FF,2)=1) then 
		U(jj):= 1;
		FF:= (FF-1)/2;
		BNM:=BNM+D+A;  #Double then Add
	else 
		U(jj):= 0;
		FF:=FF/2:
		BNM:=BNM+D:  #Double then Add
    end if;
    jj:= jj+1:
end do:
ll:=jj;


############################################################
###  NAF conversion cost of time
while E &gt; 0 do 
	if (modp(E,2)&lt;&gt;0) then 
		Z(i):= 2- modp(E,4);
		E:=E-Z(i);
     else 
     	Z(i):=0; 	
     end if;
     if (Z(i)=1 or Z(i)=-1) then 
          NAF:= NAF+A+D;
     else 
          NAF:=NAF+D;	
     end if;
     E:= E/2;    
     i:=i+1: 
end do: 
n:=i;
#############################################################
#Binary method with Add and discard cost of time
while F&gt;0 do
	if (modp(F,2)=1) then 
		Y(j):= 1;
		F:= (F-1)/2;
		BAD:=BAD+D+A;  #Double then Add
	else 
		Y(j):= 0;
		BAD:=BAD+D+A:  #Double then Add
    end if;
    j:= j+1:
end do:
l:=j;
###############################
# Montgomery ladder algorithm PARALLEL
		
for i from ll-2 by -1 to 0 do 
if Y(i)=1 then 
	R0:=R0+R1;
	R1:=2*R1;
	MGLP:=MGLP+A; # Adding takes longer than Doubling
else 
	R1:=R0+R1;
	R0:=2*R0;
	MGLP:=MGLP+A;
end if;
end do:
MGLP:= MGLP+ D;  # need to precompute R1:=2*P
###############################
###############################
# Montgomery ladder algorithm SERIAL
		
for i from ll-2 by -1 to 0 do 
if Y(i)=1 then 
	R0:=R0+R1;
	R1:=2*R1;
	MGLS:=MGLS+A+D; # Adding takes longer than Doubling
else 
	R1:=R0+R1;
	R0:=2*R0;
	MGLS:=MGLS+A+D;
end if;
end do:
MGLS := MGLS+D;  # need to precompute R1:=2*P
###############################

###############################
BNMC(m):=BNM-A-D:
BADC(m):=BAD-D-A:
NAFC(m):=NAF-D-A:   			
MGLSC(m):=MGLS:
MGLPC(m):=MGLP:

end do:


with(Statistics):
#BINA := Array([seq(BNCC(i) , i = 30 .. 50)]):
BADA := Array([seq(BADC(i) , i = 30 .. 50)]):
NAFA := Array([seq(NAFC(i), i = 30 .. 50)]):
MGLSA := Array([seq(MGLSC(i) , i = 30 .. 50)]):
MGLPA := Array([seq(MGLPC(i) , i = 30 .. 50)]):
ColumnGraph([BADA,NAFA,MGLSA,MGLPA], title = &quot;BIN -- A/DA BIN -- NAF -- S MNG --P MNG    TIMING COST&quot;, 
legend =[&quot;BA/A&quot;,&quot;NAF&quot;,&quot;SMNG&quot;,&quot;PMNG&quot;],color = [&quot;Green&quot;,&quot;Blue&quot;,&quot;Black&quot;,&quot;Red&quot;]);
